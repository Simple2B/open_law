{
  "compilerOptions": {
      "lib": ["dom", "es2015"],
      "outDir": "./app/static/js/",
      "sourceMap": true,
      "declaration": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "allowJs": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
  },
  "include": ["src/**/*.ts*", "src/wallet.js"],
  "exclude": ["node_modules", "dist", "lib"]
}
